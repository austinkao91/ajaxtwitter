<h1>Search!</h1>



<div class = "users-search" data-users-list="#users-search">
    <input
      type="text"
      name="query"
      class="search"></input>
</div>

<ul class = "users">
  <% @users.each do |user| %>
    <li>
      <%= link_to user.username, user_url(user) %>
      <%= render "follows/form", user: user %>
    </li>
  <% end %>
</ul>
